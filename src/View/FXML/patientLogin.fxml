<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1200.0" styleClass="vboxContainer" stylesheets="@../../Style/patientLogin.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.PatientLoginController">
   <children>
      <VBox alignment="CENTER" layoutX="325.0" layoutY="75.0" prefHeight="450.0" prefWidth="550.0" spacing="30.0" stylesheets="@../../Style/adminlogin.css">
         <children>
            <ImageView fx:id="patientLogo" fitHeight="126.0" fitWidth="430.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/patientlogo.png" />
               </image>
            </ImageView>
            <HBox alignment="CENTER" prefHeight="59.0" prefWidth="550.0" spacing="40.0">
               <children>
                  <Label fx:id="patientLabel" prefHeight="45.0" prefWidth="120.0" styleClass="labelStyle" text="Username" />
                  <TextField fx:id="patientField" prefHeight="57.0" prefWidth="300.0" styleClass="inputStyle" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="210.0" prefHeight="60.0" prefWidth="550.0" spacing="40.0">
               <children>
                  <Label fx:id="passwordLabel" prefHeight="49.0" prefWidth="120.0" styleClass="labelStyle" text="Password" />
                  <PasswordField fx:id="passwordField" prefWidth="300.0" styleClass="inputStyle" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="65.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#loginBtnHandel" prefHeight="65.0" prefWidth="120.0" styleClass="testButton" text="Login" />
                  <Button fx:id="backBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backBtnHandel" prefHeight="65.0" prefWidth="120.0" styleClass="backButton" text="Back" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
